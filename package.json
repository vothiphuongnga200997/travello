        "parse": "^2.10.0",
        "roboto-fontface": "0.8.0",
        "rxjs": "6.5.2",
        "rxjs-compat": "6.3.0",
        "screenfull": "^3.3.0",
        "socicon": "3.0.5",
        "tinymce": "4.5.7",
        "tslib": "^1.9.0",
        "typeface-exo": "0.0.22",
        "unescape": "1.0.1",
        "web-animations-js": "2.3.1",
        "zone.js": "~0.9.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.800.2",
        "@angular/cli": "^8.0.2",
        "@angular/compiler-cli": "^8.0.0",
        "@angular/language-service": "8.0.0",
        "@compodoc/compodoc": "^1.1.10",
        "@fortawesome/fontawesome-free": "^5.2.0",
        "@types/d3-color": "1.0.5",
        "@types/googlemaps": "^3.30.4",
        "@types/jasmine": "2.5.54",
        "@types/jasminewd2": "2.0.3",
        "@types/leaflet": "1.2.3",
        "@types/node": "6.0.90",
        "codelyzer": "^5.0.1",
        "conventional-changelog-cli": "1.3.4",
        "husky": "0.13.3",
        "jasmine-core": "2.6.4",
        "jasmine-spec-reporter": "4.1.1",
        "npm-run-all": "4.0.2",
        "prettier": "1.17.1",
        "protractor": "5.1.2",
        "rimraf": "2.6.1",
        "run-script-os": "1.0.5",
        "sonar-scanner": "3.1.0",
        "stylelint": "7.13.0",
        "ts-node": "3.2.2",
        "tslint": "^5.7.0",
        "tslint-language-service": "^0.9.9",
        "typescript": "3.4.5",
        "uuid": "3.3.2",
        "xml-beautifier": "^0.4.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run fix-prettier && npm run lint && git reset package-lock.json",
            "pre-push": "npm run build:prod && npm run sonar"
        }
    }
}
