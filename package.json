{
    "name": "spiderium-studio",
    "version": "3.1.0",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://gitlab.innoria.com/spiderium/spiderium-studio.git"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "IE 11"
    ],
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "postinstall": "node patch-webpack.js",
        "build": "ng build",
        "build:prod": "npm run build -- --prod --aot --configuration=production",
        "build:dev": "npm run build -- --configuration=development",
        "lint": "npm run lint:ts && npm run lint:styles",
        "lint:ts": "tslint --project ./",
        "lint:styles": "stylelint --fix ./src/**/*.scss",
        "sonar": "sonar-scanner",
        "fix-prettier": "prettier --write ./src/**/*.ts",
        "git-push": "run-script-os",
        "git-push:win32": "git-push.bat",
        "git-push:darwin:linux": "branch=$(git branch | sed -n -e 's/^\\* \\(.*\\)/\\1/p') && git reset package-lock.json && git commit -m $branch\\ \\#code-review && git push --set-upstream origin $branch"
    },
    "dependencies": {
        "@agm/core": "1.0.0-beta.5",
        "@angular/animations": "7.2.15",
        "@angular/cdk": "7.3.7",
        "@angular/common": "7.2.15",
        "@angular/compiler": "7.2.15",
        "@angular/core": "7.2.15",
        "@angular/forms": "7.2.15",
        "@angular/http": "7.2.15",
        "@angular/platform-browser": "7.2.15",
        "@angular/platform-browser-dynamic": "7.2.15",
        "@angular/router": "7.2.15",
        "@asymmetrik/ngx-leaflet": "3.0.1",
        "@azure/msal-angular": "^0.1.2",
        "@nebular/auth": "3.1.0",
        "@nebular/bootstrap": "3.1.0",
        "@nebular/security": "3.1.0",
        "@nebular/theme": "3.1.0",
        "@ng-bootstrap/ng-bootstrap": "4.1.2",
        "@swimlane/ngx-charts": "10.0.0",
        "angular-formio": "3.11.5",
        "angular-monaco-editor": "1.9.0",
        "angular-tree-component": "7.2.0",
        "angular2-chartjs": "0.5.1",
        "angular2-toaster": "6.1.0",
        "axios": "^0.18.0",
        "bootstrap": "4.3.1",
        "ckeditor": "4.11.4",
        "classlist.js": "1.1.20150312",
        "copy-to-clipboard": "3.2.0",
        "core-js": "2.5.1",
        "echarts": "4.0.2",
        "elasticsearch": "^16.1.0",
        "eva-icons": "1.1.0",
        "fancy-format-log": "1.0.0",
        "formiojs": "3.16.4",
        "intl": "1.2.5",
        "ionicons": "2.0.1",
        "jquery": "3.4.1",
        "kafka-proxy-rest": "1.0.7",
        "lodash": "4.17.11",
        "moment": "2.24.0",
        "monaco-editor": "0.17.0",
        "msal": "^1.0.0-preview.5",
        "nebular-icons": "1.1.0",
        "ng2-ckeditor": "1.2.2",
        "ng2-completer": "^2.0.8",
        "ng2-smart-table": "1.4.0",
        "ngx-cron-jobs": "^7.4.10",
        "ngx-echarts": "4.1.1",
        "ngx-socket-io": "2.1.1",
        "ngx-xml2json": "1.0.2",
        "normalize.css": "6.0.0",
        "pace-js": "1.0.2",
        "pako": "1.0.10",
        "parse": "^2.4.0",
        "roboto-fontface": "0.10.0",
        "rxjs": "6.5.2",
        "rxjs-compat": "6.5.2",
        "typeface-exo": "0.0.61",
        "unescape": "1.0.1",
        "web-animations-js": "2.3.1",
        "zone.js": "0.9.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "0.13.9",
        "@angular/cli": "7.3.9",
        "@angular/compiler-cli": "7.2.15",
        "@angular/language-service": "7.2.15",
        "@fortawesome/fontawesome-free": "5.8.2",
        "@types/leaflet": "1.4.4",
        "@types/node": "12.0.0",
        "codelyzer": "5.0.1",
        "husky": "^2.3.0",
        "prettier": "1.17.1",
        "run-script-os": "1.0.5",
        "sonar-scanner": "3.1.0",
        "stylelint": "10.0.1",
        "ts-node": "8.1.0",
        "tslint": "5.16.0",
        "typescript": "3.2.4",
        "uuid": "3.3.2",
        "xml-beautifier": "^0.4.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run fix-prettier && npm run lint && git reset package-lock.json",
            "pre-push": "npm run build:prod && npm run sonar"
        }
    }
}
