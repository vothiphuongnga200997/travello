{
    "name": "travello",
    "version": "1.0.0",
    "license": "MIT",
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "IE 11"
    ],
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "postinstall": "node patch-webpack.js",
        "build": "ng build",
        "build:prod": "npm run build -- --prod --aot --configuration=production",
        "build:dev": "npm run build -- --configuration=development",
        "lint": "npm run lint:ts && npm run lint:styles",
        "lint:ts": "tslint --project ./",
        "lint:styles": "stylelint --fix ./src/**/*.scss",
        "sonar": "sonar-scanner",
        "fix-prettier": "prettier --write ./src/**/*.ts",
        "git-push": "run-script-os",
        "git-push:win32": "git-push.bat",
        "git-push:darwin:linux": "branch=$(git branch | sed -n -e 's/^\\* \\(.*\\)/\\1/p') && git reset package-lock.json && git commit -m $branch\\ \\#code-review && git push --set-upstream origin $branch"
    },
    "dependencies": {
        "@agm/core": "^1.0.0-beta.5",
        "@angular/animations": "^8.0.0",
        "@angular/cdk": "^8.0.0",
        "@angular/common": "^8.0.0",
        "@angular/compiler": "^8.0.0",
        "@angular/core": "^8.0.0",
        "@angular/forms": "^8.0.0",
        "@angular/http": "^7.2.15",
        "@angular/platform-browser": "^8.0.0",
        "@angular/platform-browser-dynamic": "^8.0.0",
        "@angular/router": "^8.0.0",
        "@asymmetrik/ngx-leaflet": "3.0.1",
        "@nebular/auth": "4.1.2",
        "@nebular/eva-icons": "4.1.2",
        "@nebular/security": "4.1.2",
        "@nebular/theme": "4.1.2",
        "@ng-bootstrap/ng-bootstrap": "4.1.2",
        "@swimlane/ngx-charts": "^10.0.0",
        "angular-formio": "3.11.5",
        "angular-monaco-editor": "1.9.0",
        "angular-tree-component": "7.2.0",
        "angular2-chartjs": "0.4.1",
        "angular2-toaster": "^7.0.0",
        "axios": "^0.18.0",
        "bootstrap": "4.3.1",
        "chart.js": "^2.8.0",
        "classlist.js": "1.1.20150312",
        "copy-to-clipboard": "3.2.0",
        "core-js": "2.5.1",
        "echarts": "^4.0.2",
        "elasticsearch": "^16.1.0",
        "eva-icons": "^1.1.0",
        "fancy-format-log": "1.0.0",
        "formiojs": "3.16.4",
        "hammerjs": "^2.0.8",
        "intl": "1.2.5",
        "ionicons": "2.0.1",
        "jquery": "3.4.1",
        "leaflet": "1.2.0",
        "lodash": "^4.17.15",
        "moment": "2.24.0",
        "monaco-editor": "0.17.0",
        "msal": "^1.0.0-preview.5",
        "nebular-icons": "1.1.0",
        "ng2-ckeditor": "1.2.0",
        "ng2-completer": "2.0.8",
        "ng2-search-filter": "^0.5.1",
        "ng2-smart-table": "1.3.5",
        "ngx-carousel": "^1.3.5",
        "ngx-cron-jobs": "^7.4.10",
        "ngx-currency": "^2.0.0",
        "ngx-echarts": "^4.0.1",
        "ngx-export-as": "^1.3.0",
        "ngx-lightbox": "^2.1.0",
        "ngx-password-toggle": "^1.0.3",
        "ngx-paypal": "^5.0.0",
        "ngx-socket-io": "2.1.1",
        "ngx-xml2json": "1.0.2",
